  TfrmCommonCustom = class(TForm)
  private
    { Private declarations }
  public
    { Public declarations }
    procedure setLookAndFeel();
    procedure setLanguage();
    procedure setSkin;
  end;

var
  frmCommonCustom: TfrmCommonCustom;

implementation

{$R *.dfm}

end.
